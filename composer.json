{
    "name": "pollin14/fret",
    "description": "Fret calculator",
    "keywords": [
        "science",
        "fret"
    ],
    "homepage": "https://github.com/pollin14/fret",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pollin14/fret/issues",
        "source": "https://github.com/pollin14/fret"
    },
    "authors": [
        {
            "name": "Victor Aguilar",
            "email": "victor.aguilar@ciencias.unam.mx"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-mbstring": "*",
        "league/csv": "^9.7",
        "richjenks/stats": "^1.0"
    },
    "require-dev": {
        "laravel-zero/framework": "^8.4",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^1.0",
        "ext-intl": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/fret"
    ]
}
